"\n"

#: src/Sight.cpp:524
#, c-format
msgid ""
"Eye Height is %.3f meters\n"
"Height Correction Degrees = 1.753*sqrt(%.3f) / 60.0\n"
"Height Correction Degrees = %.3f\n"
msgstr ""
"la hauteur de l'oeil est %.3f mètres\n"
"La correction de la hauteur en degrés = 1.753*sqrt(%.3f) / 60.0\n"
"La correction de la hauteur en degré = %.3f\n"

#: src/Sight.cpp:539
#, c-format
msgid ""
"\n"
"Refraction Correction\n"
"x = tan(Pi/180*Measurement + 4.848e-2*(Pi/180) / (tan(Pi/180*Measurement) "
"+ .028))\n"
"x = tan(Pi/180*%.3f + 4.848e-2*(Pi/180) / (tan(Pi/180*%.3f) + .028))\n"
"x = %.3f\n"
msgstr ""
"\n"
"Correction de la réfraction\n"
"x = tan(Pi/180*Measurement + 4.848e-2*(Pi/180) / (tan(Pi/180*Measurement) "
"+ .028))\n"
"x = tan(Pi/180*%.3f + 4.848e-2*(Pi/180) / (tan(Pi/180*%.3f) + .028))\n"
"x = %.3f\n"

#: src/Sight.cpp:544
#, c-format
msgid ""
"RefractionCorrection = .267 * Pressure / (x*(Temperature + 273.15)) / 60.0\n"
"RefractionCorrection = .267 * %.3f / (x*(%.3f + 273.15)) / 60.0\n"
"RefractionCorrection = %.5f\n"
msgstr ""
"Correction de la réfraction = .267 * Pressure / (x*(Temperature + 273.15)) / "
"60.0\n"
"Correction de la réfraction = .267 * %.3f / (x*(%.3f + 273.15)) / 60.0\n"
"Correction de la réfraction = %.5f\n"

#: src/Sight.cpp:559
#, c-format
msgid ""
"\n"
"Sun selected, Limb Correction\n"
"ra = %.3f, lc = 0.266564/ra = %.3f\n"
msgstr ""
"\n"
"Sun selected, Limb Correction\n"
"ra = %.3f, lc = 0.266564/ra = %.3f\n"

#: src/Sight.cpp:569
#, c-format
msgid ""
"\n"
"Moon selected, Limb Correction\n"
"SD = %.3f\n"
"lc = 180/Pi * asin(Pi/180*SD)\n"
"lc = %.3f\n"
msgstr ""
"\n"
"Moon selected, Limb Correction\n"
"SD = %.3f\n"
"lc = 180/Pi * asin(Pi/180*SD)\n"
"lc = %.3f\n"

#: src/Sight.cpp:579
msgid "Upper Limb"
msgstr "Upper Limb"

#: src/Sight.cpp:582
msgid "Lower Limb"
msgstr "Lower Limb"

#: src/Sight.cpp:585
#, c-format
msgid ""
"\n"
"LimbCorrection = %.5f\n"
msgstr ""
"\n"
"LimbCorrection = %.5f\n"

#: src/Sight.cpp:590
#, c-format
msgid ""
"\n"
"Observed Altitude\n"
"ObservedAltitude = Measurement - IndexCorrection - EyeHeightCorrection - "
"RefractionCorrection - LimbCorrection\n"
"ObservedAltitude = %.3f - %.3f - %.3f - %.3f - %.3f\n"
"ObservedAltitude = %.5f\n"
msgstr ""
"\n"
"Altitude observée\n"
"Altitude observée = Measurement - IndexCorrection - EyeHeightCorrection - "
"RefractionCorrection - LimbCorrection\n"
"Altitude observée = %.3f - %.3f - %.3f - %.3f - %.3f\n"
"Altitude observée = %.5f\n"

#: src/Sight.cpp:605
#, c-format
msgid ""
"\n"
"Sun selected, parallax correction\n"
"rad = %.3f, HP = 0.002442/rad = %.3f\n"
msgstr ""
"\n"
"Le soleil est sélectionné, correction du paralaxe\n"
"rad = %.3f, HP = 0.002442/rad = %.3f\n"

#: src/Sight.cpp:614
#, c-format
msgid ""
"\n"
"Moon selected, parallax correction\n"
"HP = %.3f\n"
msgstr ""
"\n"
"La lune est sélectionnée, correction du paralaxe\n"
"HP = %.3f\n"

#: src/Sight.cpp:620
#, c-format
msgid ""
"ParallaxCorrection = -180/Pi * asin( sin(Pi/180 * HP ) * cos(Pi/180 * "
"ObservedAltitude))\n"
"ParallaxCorrection = -180/Pi * asin( sin(Pi/180 * %.3f ) * cos(Pi/180 * "
"%.3f))\n"
"ParallaxCorrection = %.5f\n"
msgstr ""
"Correction du paralaxe = -180/Pi * asin( sin(Pi/180 * HP ) * cos(Pi/180 * "
"ObservedAltitude))\n"
"Correction du paralaxe = -180/Pi * asin( sin(Pi/180 * %.3f ) * cos(Pi/180 * "
"%.3f))\n"
"Correction du paralaxe = %.5f\n"

#: src/Sight.cpp:627
#, c-format
msgid ""
"\n"
"Corrected Altitude\n"
"CorrectedAltitude = ObservedAltitude - ParallaxCorrection\n"
"CorrectedAltitude = %.3f - %.3f\n"
"CorrectedAltitude = %.5f\n"
msgstr ""
"\n"
"Altitude corrigée\n"
"Altitude corrigée = Altitude observée - Correction du parallaxe\n"
"Altitude corrigée = %.3f - %.3f\n"
"altitude corrigée = %.5f\n"

